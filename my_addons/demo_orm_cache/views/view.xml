<?xml version="1.0"?>
<odoo>
  <record id="view_form_demo_cache" model="ir.ui.view">
    <field name="name">Demo Cache Form</field>
    <field name="model">demo.cache</field>
    <field name="arch" type="xml">
      <form>
        <header>
          <button name="demo_ormcache" string="demo ormcache" type="object" class="oe_highlight o_expense_sheet_submit"/>
          <button name="demo_ormcache_by_env" string="demo ormcache by env" type="object" class="oe_highlight o_expense_sheet_submit"/>
          <button name="demo_ormcache_context" string="demo ormcache context" type="object" class="oe_highlight o_expense_sheet_submit"/>
          <button name="demo_clear_cache" string="demo clear cache" type="object" class="oe_highlight o_expense_sheet_submit"/>
        </header>
        <group>
          <field name="name"/>
        </group>
      </form>
    </field>
  </record>

  <record id="view_tree_demo_cache" model="ir.ui.view">
    <field name="name">Demo Cache List</field>
    <field name="model">demo.cache</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
      </tree>
    </field>
  </record>

</odoo>